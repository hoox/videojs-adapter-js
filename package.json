{
  "name": "youbora-adapter-videojs",
  "version": "6.4.2",
  "main": "src/adapter.js",
  "description": "Adapter for NPAW's youboralib",
  "scripts": {
    "build": "npm run manifest && webpack -p",
    "watch": "npm run manifest && webpack -p --progress --color --watch",
    "deployable": "npm run build && youbora-util deployable",
    "manifest": "youbora-util manifest",
    "preversion": "npm run build",
    "clean": "rm -rf dist deploy manifest.json"
  },
  "dependencies": {
    "youboralib": "^6.4.11"
  },
  "devDependencies": {
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "youboralib-util": "1.3.X"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/npaw/videojs-adapter-js.git"
  },
  "author": "Jordi Aguilar",
  "license": "MIT",
  "bugs": {
    "email": "support@nicepeopleatwork.com"
  }
}
